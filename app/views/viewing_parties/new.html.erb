<h3>@movie.title Viewing Party</h3>
  <%= form_with model: @viewing_party, local: true do |f| %>  
    <p><%= f.label :movie_title, 'Movie title:' %></p>
    <p><%=  f.text_field :movie_title%></p>

    <p><%= f.label :duration, 'Duration in minutes:' %></p>
    <p><%=  f.text_field :duration%></p> 

    <p><%= f.label :event_date, 'Date for event:' %></p>
    <p><%=  f.text_field :event_date%></p>

    <p><%= f.label :start_time, 'Start time:' %></p>
    <p><%=  f.text_field :start_time%></p>

    <p><%= f.label :name, 'Name for event:' %></p>
    <p><%=  f.text_field :name%></p>

    <p><%= f.submit "Create" %></p>
  <% end %>